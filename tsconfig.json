{
  "include": [
    "index.ts",
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": false,
    "composite": false,
    "outDir": "server/",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
  }
}