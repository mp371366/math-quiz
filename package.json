{
  "name": "math-quiz",
  "version": "1.0.0",
  "description": "Math quiz app",
  "main": "index.ts",
  "scripts": {
    "start": "npx ts-node index.ts",
    "build": "cd scripts/ && tsc",
    "lint": "tslint **/*.ts",
    "clean": "rm -rf build/",
    "base:make": "npm run base:clean && npx ts-node base/init.ts",
    "base:insert": "npx ts-node base/insert.ts",
    "base:clean": "rm -f base.db",
    "createdb": "npm run base:make && npm run base:insert"
  },
  "keywords": [
    "math",
    "quiz"
  ],
  "author": "mp371366",
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "pug": "^3.0.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/node": "^14.6.0",
    "@types/sqlite3": "^3.1.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}