{
  "name": "math-quiz",
  "version": "1.0.0",
  "description": "Math quiz app",
  "main": "index.ts",
  "scripts": {
    "start": "npx node server/index.js",
    "build": "npm run build:back-end && npm run build:front-end",
    "build:front-end": "cd scripts/ && tsc",
    "build:back-end": "tsc",
    "lint": "tslint **/*.ts",
    "test": "npx mocha -r ts-node/register tests/*.ts && npm run createdb",
    "clean": "rm -rf build/ server/",
    "base:make": "npm run base:clean && npx ts-node base/init.ts",
    "base:insert": "npx ts-node base/insert.ts",
    "base:clean": "rm -f base.db",
    "createdb": "npm run base:make && npm run base:insert"
  },
  "keywords": [
    "math",
    "quiz"
  ],
  "author": "mp371366",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "pug": "^3.0.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.12",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/selenium-webdriver": "^4.0.9",
    "@types/sqlite3": "^3.1.6",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "mocha-webdriver": "^0.2.8",
    "node": "^14.8.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
